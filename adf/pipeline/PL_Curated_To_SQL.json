{
	"name": "PL_Curated_To_SQL",
	"properties": {
		"activities": [
			{
				"name": "Copy to SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "*.parquet",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "ParquetReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Timestamp",
									"type": "DateTime",
									"physicalType": "INT96"
								},
								"sink": {
									"name": "Timestamp",
									"type": "DateTime",
									"physicalType": "datetime"
								}
							},
							{
								"source": {
									"name": "SensorID",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "SensorID",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "VehicleCount",
									"type": "Int32",
									"physicalType": "INT_32"
								},
								"sink": {
									"name": "VehicleCount",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "AvgSpeed",
									"type": "Int32",
									"physicalType": "INT_32"
								},
								"sink": {
									"name": "AvgSpeed",
									"type": "Double",
									"physicalType": "float"
								}
							},
							{
								"source": {
									"name": "TrafficDensity",
									"type": "Double",
									"physicalType": "DOUBLE"
								},
								"sink": {
									"name": "TrafficDensity",
									"type": "Double",
									"physicalType": "float"
								}
							},
							{
								"source": {
									"name": "CongestionLevel",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "CongestionLevel",
									"type": "String",
									"physicalType": "nvarchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_curated_traffic",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_sql_facttraffic",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}