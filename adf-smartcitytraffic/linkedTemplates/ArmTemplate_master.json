{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-smartcitytraffic"
		},
		"LS_ADLS_SmartCity_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_SmartCity'"
		},
		"LS_ASD_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ASD'"
		},
		"LS_ASQLDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_ASQLDB'"
		},
		"LS_ADLS_SmartCity_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://smartcitytrafficdl.dfs.core.windows.net/"
		},
		"LS_ASD_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://smartcitytrafficdl.dfs.core.windows.net/"
		},
		"LS_ASQLDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "smartcity-sql-server.database.windows.net"
		},
		"LS_ASQLDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SmartCityTrafficDB"
		},
		"LS_ASQLDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqlserver"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-smartcitytraffic_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_SmartCity_accountKey": {
						"value": "[parameters('LS_ADLS_SmartCity_accountKey')]"
					},
					"LS_ASD_accountKey": {
						"value": "[parameters('LS_ASD_accountKey')]"
					},
					"LS_ASQLDB_password": {
						"value": "[parameters('LS_ASQLDB_password')]"
					},
					"LS_ADLS_SmartCity_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_SmartCity_properties_typeProperties_url')]"
					},
					"LS_ASD_properties_typeProperties_url": {
						"value": "[parameters('LS_ASD_properties_typeProperties_url')]"
					},
					"LS_ASQLDB_properties_typeProperties_server": {
						"value": "[parameters('LS_ASQLDB_properties_typeProperties_server')]"
					},
					"LS_ASQLDB_properties_typeProperties_database": {
						"value": "[parameters('LS_ASQLDB_properties_typeProperties_database')]"
					},
					"LS_ASQLDB_properties_typeProperties_userName": {
						"value": "[parameters('LS_ASQLDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}